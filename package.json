{
  "name": "Test Application",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:headless:no-watch": "ng test --browsers ChromeHeadless --watch=false",
    "lint": "ng lint",
    "lint-staged": "node ng-lint-staged.js",
    "format:fix": "pretty-quick --staged --verbose",
    "precommit": "run-s format:fix lint-staged",
    "prettier": "prettier --config ./.prettierrc --list-different --end-of-line auto \"src/{app,environments}/**/*{.ts,.html,.scss}\"",
    "prettier:fix": "prettier --config ./.prettierrc --write \"src/{app,environments}/**/*{.ts,.html,.scss}\"",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:fix lint-staged"
    }
  },
  "private": true,
  "dependencies": {
