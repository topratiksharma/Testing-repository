    "ag-grid-community": "^30.2.0",
    "ag-grid-enterprise": "^30.2.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "i": "^0.3.7",
    "jwt-decode": "^3.1.2",
